#!/usr/bin/env python

home_goals = input()
home_points = input()
away_goals = input()
away_points = input()

home_score = home_goals * (7 + home_points)
away_score = away_goals * (7 + away_points)

if away_score == home_score:
	print 'draw'
elif home_score < away_score:
	print 'away win'
elif away_score < home_score:
	print 'home win'
